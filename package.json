  "scripts": {
    "test": "karma start karma.dev.conf.js",
    "test-file": "./build/test.sh",
    "test-cleanup": "./build/test_cleanup.sh",
    "debug-file": "./build/debug.sh",
    "coverage": "COVERAGE=1 karma start karma.dev.conf.js",
    "sync": "./build/push_to_dev.sh",
    "dev": "./node_modules/.bin/webpack --progress --colors",
    "jenkins": "BUILD_CI=1 node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --progress --colors",
    "prod": "BUILD_PROD=1 node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js",
    "watch": "npm run watch-rb & ./node_modules/.bin/webpack --watch",
    "clean": "rm -rf dist && rm -rf reports && rm -rf src/i18n/json",
    "watch-rb": "mojito-rb-gen -w -s src/i18n -o src/i18n/json -b en-US.properties",
    "build-rb": "mojito-rb-gen -s src/i18n -o src/i18n/json -b en-US.properties",
    "lint": "./node_modules/.bin/eslint src/lib",
    "prepush": "npm run lint",
    "build": "npm run clean && npm run build-rb && npm run lint && npm run dev",
    "ci": "npm run clean && npm run build-rb && npm run lint && node --max_old_space_size=4096 ./node_modules/.bin/gulp test-release && npm run jenkins",
    "release": "npm run clean && npm run build-rb && npm run lint && node --max_old_space_size=4096 ./node_modules/.bin/gulp test-release && npm run prod"
  }
